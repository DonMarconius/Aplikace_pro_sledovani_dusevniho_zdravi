<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.MAIN.Rozcestnik.Testy.Rozcestnik_testy"
             Title="Rozcestník dotazníků">

    <ScrollView>
        <VerticalStackLayout
            Spacing="25"
            Padding="30,0"
            VerticalOptions="Center">

            <Button
                x:Name="DepreseBtn"
                Text="Deprese"
                Clicked="DepreseBtn_Clicked"
                HorizontalOptions="Center" />

            <Button
                x:Name="UzkostiBtn"
                Text="Úzkosti"
                Clicked="UzkostiBtn_Clicked"
                HorizontalOptions="Center" />

            <Button
                x:Name="MainBtn"
                Text="Zpět"
                Clicked="MainBtn_Clicked"
                HorizontalOptions="Center" />

        </VerticalStackLayout>
    </ScrollView>

</ContentPage>